var html = "<%= j render :partial => 'chat/message', collection: @chats %>";
$('#chat-plugin-messages').prepend(html);
$.ajax({
    contentType:"application/octet-stream",
    type:"GET",
    data: {last: $("#chat-plugin-messages > div:first").data('messageid')},
    url: "<%= url_for :controller => 'chat', :action => 'list', :project => @project %>"
});