var html = "<%= render :partial => 'chat/message', collection: @chats %>";
$('#chat-plugin-messages').empty().append(html);
$.ajax({
    method:"GET",
    data: {last: $("#chat-plugin-messages").firstChild().data('messageid')},
    url: "<%= url_for :controller => 'chat', :action => 'list', :project => @project %>"
});